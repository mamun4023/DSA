<script type="text/javascript">

function Binary_Search(arr, x, low, high){
	while(low<=high){
		let mid = Math.floor((low+high)/2);

		if(arr[mid] == x){
			return true
		}
		if(x < arr[mid]){
			high = mid-1
		}else{
			low = mid+1
		}
	}
	return false;
}





var arr = [1,2,3,4,5];
let low = 0;
let high = arr.length;
let x = 5;
console.log(Binary_Search(arr, x, low, high))


 






</script>
