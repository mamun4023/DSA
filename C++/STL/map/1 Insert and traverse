#include <iostream>
#include <map>

using namespace std;

int main()
{

map<string, int> s;

s["one"] = 1;
s["two"] = 2;
s["three"] = 3;


map<string, int>::iterator it = s.begin();

while (it != s.end()){
	cout << "Key: " << it->first << ", Value: " << it->second << std::endl;
	++it;
}

return 0;
}
