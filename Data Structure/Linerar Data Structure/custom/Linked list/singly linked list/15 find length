
<script type="text/javascript">


class Node{
	constructor(data){
		this.data = data;
		this.next = null;
	}
}


class LinkedList{
	constructor(){
		this.head = null;
	}

	insert(data){
		let newNode = new Node(data);
		if(this.head === null){
			this.head = newNode;
		}else{
			let traverse = this.head;
			while(traverse.next){
				traverse = traverse.next;
			}

			traverse.next = newNode;

		}
	}

	display(){
		let traverse = this.head;
		while(traverse){
			document.write(traverse.data+ " ")
			traverse  = traverse.next;
		}
	}

	length(){
		let traverse = this.head;
		let count = 1;
		while(traverse.next){
			count++;
			traverse = traverse.next;
		}
	return count;	
  }    
}


let obj = new LinkedList();
obj.insert(10);
obj.insert(3);
obj.insert(44);
obj.insert(5);
obj.insert(5);
obj.insert(5);
obj.display();
console.log(obj.length());



</script>
