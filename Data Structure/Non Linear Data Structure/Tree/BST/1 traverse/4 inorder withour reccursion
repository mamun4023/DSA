	this.WithoutRec = (node)=>{
		if(!node) 
			return;

		var stack = [];
		

		while(node != null || stack.length > 0){
			// reach left most
			while(node != null){
				stack.push(node);
				node = node.left;
			}
			node = stack.pop();
			document.write(node.data + " ");
			node = node.right;
		}
	}
