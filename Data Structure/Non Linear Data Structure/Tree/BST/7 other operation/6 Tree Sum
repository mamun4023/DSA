	treeSum(node){
		if(node == null)
			return 0;
		else{
			let leftSum = this.treeSum(node.left);
			let rightSum = this.treeSum(node.right);

			return node.data + leftSum + rightSum
		}
	}



// more professional

treeSum(node) {
    if (node === null) return 0;
    return node.data + this.treeSum(node.left) + this.treeSum(node.right);
}
